{
  "plugins": [
    "stylelint-scss"
  ],
  "extends": [
    "stylelint-config-rational-order",
    "./node_modules/prettier-stylelint/config.js"
  ],
  "rules": {
    "indentation": 2,
    "declaration-colon-newline-after": null,
    "string-quotes": "single",
    "value-list-comma-newline-after": "never-multi-line"
  }
}